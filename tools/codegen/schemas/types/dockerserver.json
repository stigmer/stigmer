{
  "name": "DockerServer",
  "description": "DockerServer defines an MCP server that runs in a Docker container.",
  "protoType": "ai.stigmer.agentic.agent.v1.DockerServer",
  "protoFile": "apis/ai/stigmer/agentic/agent/v1/spec.proto",
  "fields": [
    {
      "name": "Image",
      "jsonName": "image",
      "protoField": "image",
      "type": {
        "kind": "string"
      },
      "description": "Docker image name and tag.\n Example: \"ghcr.io/org/mcp-server:latest\"",
      "required": false
    },
    {
      "name": "Args",
      "jsonName": "args",
      "protoField": "args",
      "type": {
        "kind": "array",
        "elementType": {
          "kind": "string"
        }
      },
      "description": "Container command arguments (optional).\n Overrides the default CMD in the Docker image.",
      "required": false
    },
    {
      "name": "EnvPlaceholders",
      "jsonName": "envPlaceholders",
      "protoField": "env_placeholders",
      "type": {
        "kind": "map",
        "keyType": {
          "kind": "string"
        },
        "valueType": {
          "kind": "string"
        }
      },
      "description": "Environment variable placeholders (same as StdioServer).\n Example: {\"GITHUB_TOKEN\": \"${GITHUB_TOKEN}\"}",
      "required": false
    },
    {
      "name": "Volumes",
      "jsonName": "volumes",
      "protoField": "volumes",
      "type": {
        "kind": "array",
        "elementType": {
          "kind": "message",
          "messageType": "VolumeMount"
        }
      },
      "description": "Volume mounts for the container (optional).",
      "required": false
    },
    {
      "name": "Network",
      "jsonName": "network",
      "protoField": "network",
      "type": {
        "kind": "string"
      },
      "description": "Docker network to attach the container to (optional, default: \"bridge\").",
      "required": false
    },
    {
      "name": "Ports",
      "jsonName": "ports",
      "protoField": "ports",
      "type": {
        "kind": "array",
        "elementType": {
          "kind": "message",
          "messageType": "PortMapping"
        }
      },
      "description": "Port mappings for the container (optional).",
      "required": false
    },
    {
      "name": "ContainerName",
      "jsonName": "containerName",
      "protoField": "container_name",
      "type": {
        "kind": "string"
      },
      "description": "Container name (optional, auto-generated if not provided).",
      "required": false
    }
  ]
}